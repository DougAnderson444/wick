#!/usr/bin/env vinox
---
version: "0"
default_schematic: "logger"
network:
  schematics:
    - name: logger
      components:
        logger:
          id: vino-v0::log
      connections:
        - from:
            instance: <input>
            port: schem_input
          to:
            instance: logger
            port: input
        - from:
            instance: logger
            port: output
          to:
            instance: <output>
            port: schem_output
