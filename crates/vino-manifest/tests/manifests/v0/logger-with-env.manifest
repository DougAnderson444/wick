default_schematic: "logger"

version: "0"

network {
    schematics: [
      {
        name: "name_$TEST_ENV_VAR",
        instances {
          logger {
            id: "vino::log"
          }
        },
        connections: [
            {
              from { instance: "vino::schematic_input", port: "input" },
              to { instance: "logger", port: "input" },
            },
            {
              from { instance: "logger", port: "output" },
              to { instance: "vino::schematic_input", port: "output" },
            },
          ],
      }
    ],
  }



