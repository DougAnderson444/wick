#!/usr/bin/env vinox
---
version: "0"
default_schematic: "logger"
network:
  schematics:
    - name: logger
      components:
        logger:
          id: vino-v0::log
      connections:
        - from:
            reference: <input>
            port: input
          to:
            reference: logger
            port: input
        - from:
            reference: logger
            port: output
          to:
            reference: <output>
            port: output
